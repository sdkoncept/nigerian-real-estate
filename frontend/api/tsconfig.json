{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "resolveJsonModule": true,
    "types": ["@vercel/node"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}

